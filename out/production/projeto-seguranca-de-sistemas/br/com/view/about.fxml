<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="300.0" prefWidth="450.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.102-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.controller.About">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" styleClass="backgroundBlue">
         <children>
            <Label text="Sistema de Autenticação" textFill="WHITE">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding></HBox>
      <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" spacing="5.0" VBox.vgrow="ALWAYS">
         <children>
            <Label text="Atividade Avaliativa de Segurança de Sistemas">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <HBox />
            <GridPane hgap="5.0">
              <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="160.0" minWidth="160.0" prefWidth="50.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="IP:" />
                  <Label fx:id="LBip" text="Label" GridPane.columnIndex="1" />
                  <Label text="Endereço de hardware:" GridPane.rowIndex="1" />
                  <Label fx:id="LBmac" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <Label text="sistema de autenticação versão 1.0" />
         </children>
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding></VBox>
      <Label text="2016-09">
         <font>
            <Font size="11.0" />
         </font></Label>
   </children>
   <padding>
      <Insets bottom="5.0" />
   </padding>
</VBox>
